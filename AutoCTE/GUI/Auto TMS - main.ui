              <object class="ttk.Button" id="button_Concluir">
                <property name="command">autoConfirma_Clicked</property>
                <property name="text" translatable="yes">Confirmar Agend.</property>
                <property name="width">20</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Entry" id="txb_qtdeViag">
                <property name="cursor">arrow</property>
                <property name="justify">left</property>
                <property name="text" translatable="yes">Qtde. de Viagens</property>
                <property name="textvariable">string:qtdeViag</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">1</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Entry" id="txb_codViag">
                <property name="justify">left</property>
                <property name="text" translatable="yes">Cód. de Viagem</property>
                <property name="textvariable">string:codViag</property>
                <layout>
