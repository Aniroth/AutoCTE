    <child>
      <object class="ttk.Label" id="lb_Path">
        <property name="text" translatable="yes">U:\AutoCTE\LISTA.xlsx</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">10</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Button" id="button_AbrirExcel">
        <property name="command">abrirExcel</property>
        <property name="cursor">hand1</property>
        <property name="text" translatable="yes">Abrir LISTA</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">10</property>
          <property name="sticky">w</property>
        </layout>
