<?xml version='1.0' encoding='utf-8'?>
<interface>
  <object class="ttk.Frame" id="mainWindow">
    <property name="borderwidth">10</property>
    <property name="height">200</property>
    <property name="padding">0</property>
    <property name="width">200</property>
    <layout>
      <property name="column">0</property>
      <property name="propagate">True</property>
      <property name="row">12</property>
      <rows>
        <row id="9">
          <property name="minsize">30</property>
        </row>
      </rows>
    </layout>
    <child>
      <object class="ttk.Frame" id="titleFrame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">0</property>
        </layout>
        <child>
          <object class="ttk.Label" id="title">
            <property name="font">{Arial CYR} 20 {bold}</property>
            <property name="text" translatable="yes">AutoTMS</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">0</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="buttonsFrame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout>
          <property name="column">0</property>
          <property name="propagate">True</property>
          <property name="row">1</property>
          <property name="sticky">w</property>
          <rows>
            <row id="0">
              <property name="minsize">0</property>
            </row>
            <row id="1">
              <property name="minsize">0</property>
            </row>
            <row id="2">
              <property name="minsize">0</property>
            </row>
            <row id="3">
              <property name="minsize">0</property>
            </row>
            <row id="4">
              <property name="minsize">0</property>
            </row>
            <row id="5">
              <property name="minsize">0</property>
            </row>
          </rows>
        </layout>
        <child>
          <object class="ttk.Labelframe" id="lbframeCTEKlabin">
            <property name="height">0</property>
            <property name="padding">5</property>
            <property name="text" translatable="yes">CTE - Klabin</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">1</property>
              <property name="sticky">w</property>
              <rows>
                <row id="0">
                  <property name="minsize">0</property>
                </row>
                <row id="1">
                  <property name="minsize">25</property>
                </row>
              </rows>
            </layout>
            <child>
              <object class="ttk.Button" id="button_Auto_CTE">
                <property name="command">autoCTE_Clicked</property>
                <property name="text" translatable="yes">CTE - Klabin</property>
                <property name="width">20</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="lb_Klabin">
                <property name="text" translatable="yes">Emissão automática de CTE - Klabin</property>
                <property name="width">50</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Labelframe" id="lbframeCTE">
