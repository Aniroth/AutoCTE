            <property name="padding">5</property>
            <property name="text" translatable="yes">Agendamento Automático</property>
            <property name="width">200</property>
            <layout>
              <property name="column">0</property>
              <property name="propagate">True</property>
              <property name="row">3</property>
              <property name="sticky">w</property>
              <rows>
                <row id="0">
                  <property name="minsize">0</property>
                </row>
                <row id="1">
                  <property name="minsize">40</property>
                </row>
              </rows>
            </layout>
            <child>
              <object class="ttk.Button" id="button_Auto_Agendamento">
                <property name="command">autoAgendamento_Clicked</property>
                <property name="text" translatable="yes">Agendamento</property>
                <property name="width">20</property>
                <layout>
                  <property name="column">0</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Label" id="lb_Agendamento">
                <property name="anchor">w</property>
                <property name="text" translatable="yes">Agendamento automático</property>
                <property name="width">29</property>
                <layout>
                  <property name="column">1</property>
                  <property name="propagate">True</property>
                  <property name="row">0</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
